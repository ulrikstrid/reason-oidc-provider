{
  "name": "reason-oidc-provider",
  "version": "0.1.0",
  "description": "Example Reason Esy Project",
  "license": "MIT",
  "esy": {
    "build": "refmterr dune build"
  },
  "scripts": {
    "test": "esy dune runtest",
    "run": "esy x main.exe",
    "format": "esy dune build @fmt -p #{self.name}",
    "format:watch": "esy dune build --watch @fmt -p #{self.name}",
    "watch": "esy dune build --watch -p #{self.name}"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/containers": "*",
    "@opam/dune": "*",
    "@opam/fmt": "*",
    "@opam/httpaf-lwt-unix": "*",
    "@opam/httpaf": "*",
    "@opam/hpack": "*",
    "@opam/h2": "*",
    "@opam/h2-lwt": "*",
    "@opam/h2-lwt-unix": "*",
    "@opam/jwt": "*",
    "@opam/lambda-term": "*",
    "@opam/logs": "*",
    "@opam/lwt": "*",
    "@opam/yojson": "*",
    "@reason-native/console": "*",
    "@opam/uri": "*",
    "ocaml": "~4.6.0",
    "refmterr": "*",
    "@opam/tls": "*",
    "@opam/ppx_deriving": "*",
    "@opam/ppx_sexp_conv": "*",
    "@opam/x509": "0.6.1",
    "@opam/base64": "< 3.0.0",
    "@opam/cstruct-lwt": "*",
    "@opam/irmin-mem": "*",
    "@opam/uuidm": "*",
    "@opam/nocrypto": "*",
    "@opam/zarith": "*",
    "@reason-native/rely": "*"
  },
  "resolutions": {
    "@opam/httpaf": "anmonteiro/httpaf:httpaf.opam#6d2c80e",
    "@opam/httpaf-lwt-unix": "anmonteiro/httpaf:httpaf-lwt-unix.opam#6d2c80e",
    "@opam/jwt": "AestheticIntegration/ocaml-jwt:jwt.opam#1517838",
    "@opam/nocrypto": "mirleft/ocaml-nocrypto:opam#de1578f",
    "@opam/conf-gmp": {
      "source": "no-source:",
      "override": {
        "dependencies": {
          "esy-gmp": "*"
        }
      }
    },
    "esy-gmp": "esy-packages/esy-gmp#a945c56"
  },
  "devDependencies": {
    "@opam/merlin": "*",
    "ocaml": "~4.6.0"
  }
}
